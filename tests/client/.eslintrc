{

  "env": {

    // Add browser environment globals
    "browser": true,

    // Add mocha environment globals
    "mocha": true

  },
  "globals": {

    // Add angular to environment globals but do not authorize to overwrite it
    angular: false,
    inject: false,

    // Add chai to environment globals but do not authorize to overwrite it
    chai: false,
    assert: false,

    // Add protractor environment globals but do not authorize to overwrite it
    "element": true,
    "by": true,
    "protractor": true,
    "browser": true

  },
  "rules": {

    // JSDoc comments not required
    "require-jsdoc": 0,

    // Strict mode not required
    "strict": 0,

    // Google Youtube API uses non camelcase properties
    "camelcase": 0,

  }

}